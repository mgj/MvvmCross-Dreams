<VSTemplate Version="3.0.6" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>MvvmCross DREAMS Core project</Name>
    <Description>MvvmCross DREAMS Core</Description>
    <DefaultName>Dreams.Core</DefaultName>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>sw-file-icon.png</Icon>
    <TemplateID>$guid1$</TemplateID>
    <!-- Indicates how many parent folders this item template should appear in -->
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="$safeprojectname$.csproj" File="Dreams.Core.csproj" ReplaceParameters="true">
    </Project>
    <!--
    <CustomParameters>
      <CustomParameter Name="$TemplateName$" Value="Add template name here" />
      <CustomParameter Name="$TemplateID$" Value="Add GUID here" />
	    <CustomParameter Name="$TemplateType$" Value="Item" />
    </CustomParameters>
    -->
  </TemplateContent>
  <WizardExtension>
    <Assembly>NuGet.VisualStudio.Interop, Version=1.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
    <FullClassName>NuGet.VisualStudio.TemplateWizard</FullClassName>
    <WizardData>
      <packages>
        <package id="artm.MvxPlugins.Dialog" version="2.0.15" targetFramework="portable45-net45+win8" />
        <package id="artm.MvxPlugins.Fetcher" version="2.0.15" targetFramework="portable45-net45+win8" />
        <package id="artm.MvxPlugins.Logger" version="2.0.15" targetFramework="portable45-net45+win8" />
        <package id="DotNetCross.Memory.Unsafe" version="0.2.2" targetFramework="portable45-net45+win8" />
        <package id="Fody" version="1.29.4" targetFramework="portable45-net45+win8" developmentDependency="true" />
        <package id="modernhttpclient" version="2.4.2" targetFramework="portable45-net45+win8" />
        <package id="MvvmCross" version="4.2.3" targetFramework="portable45-net45+win8" />
        <package id="MvvmCross.Binding" version="4.2.3" targetFramework="portable45-net45+win8" />
        <package id="MvvmCross.Core" version="4.2.3" targetFramework="portable45-net45+win8" />
        <package id="MvvmCross.Platform" version="4.2.3" targetFramework="portable45-net45+win8" />
        <package id="MvvmCross.StarterPack" version="4.2.3" targetFramework="portable45-net45+win8" />
        <package id="Newtonsoft.Json" version="9.0.1" targetFramework="portable45-net45+win8" />
        <package id="Polly-Signed" version="4.3.0" targetFramework="portable45-net45+win8" />
        <package id="Realm" version="0.77.2" targetFramework="portable45-net45+win8" developmentDependency="true" />
        <package id="StyleCop.Analyzers" version="1.0.0" targetFramework="portable45-net45+win8" developmentDependency="true" />
